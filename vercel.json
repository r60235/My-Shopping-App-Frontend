{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(assets|images|static|vite.svg|.*\\..*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "https://my-shopping-app-backend.vercel.app/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}